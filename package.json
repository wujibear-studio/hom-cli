{
  "author": "wujibear @wujibear",
  "bin": {
    "hom": "bin/run.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^3.0.9",
    "@oclif/plugin-plugins": "^4",
    "@oclif/plugin-version": "^2.0.11",
    "liquidjs": "^10.10.0"
  },
  "description": "HÃ¶M, the CLI for shell management",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wujibear-studio/hom-cli",
  "license": "MIT",
  "main": "bin/run.js",
  "name": "hom-cli",
  "oclif": {
    "bin": "hom",
    "dirname": "hom",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found"
    ],
    "jitPlugins": {},
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wujibear-studio/hom-cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "pack:this": "oclif pack",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.2",
  "bugs": {
    "url": "https://github.com/wujibear-studio/hom-cli/issues"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module",
  "volta": {
    "node": "20.10.0",
    "yarn": "3.7.0"
  },
  "directories": {
    "test": "test"
  }
}
