{
  "author": "wujibear @wujibear",
  "bin": {
    "hom": "bin/run.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^3.0.9",
    "@oclif/plugin-plugins": "^4",
    "@oclif/plugin-update": "^4.1.8",
    "@oclif/plugin-version": "^2.0.11",
    "cli-table": "^0.3.11",
    "figlet": "^1.7.0",
    "liquidjs": "^10.10.0",
    "lolcatjs": "^2.4.3"
  },
  "description": "HÃ¶M, the shell management CLI",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "prettier": "3.5.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wujibear-studio/hom-cli",
  "license": "MIT",
  "main": "bin/run.js",
  "name": "hom-cli",
  "oclif": {
    "bin": "hom",
    "dirname": "hom-cli",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world"
      }
    },
    "macos": {
      "identifier": "com.github.wujibear.hom-cli"
    },
    "hooks": {
      "init": "./dist/src/hooks/init/check-shell"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wujibear-studio/hom-cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "pack:this": "oclif pack",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.23",
  "bugs": {
    "url": "https://github.com/wujibear-studio/hom-cli/issues"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module",
  "volta": {
    "node": "20.10.0",
    "yarn": "3.7.0"
  },
  "packageManager": "yarn@3.7.0",
  "directories": {
    "test": "test"
  }
}
